<%= simple_form_for (@building) do |f| %>

  <%= f.input :street_address%>
  <%= f.input :city%>
  <%= f.input :state %>
  <%= f.input :postal_code %>
  <%= f.input :description, required: false %>
  <%= f.association :owner, collection: Owner.all, label_method: :full_name, value_method: :id%>
  <%= f.button :submit, "Create New Building"  %>
<%end%>
